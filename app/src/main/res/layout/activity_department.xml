<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="morelife.pointguard.Schools">

    <ListView
        android:id="@+id/idListView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/appBarLayout" />

    <TextView
        android:id="@+id/fragmentText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/logoutt"
        android:layout_marginTop="14dp"
        android:layout_toStartOf="@+id/forward"
        android:layout_toLeftOf="@+id/forward"
        android:text="Image Upload"
        android:padding="3dp"
        android:layout_marginRight="10dp"
        android:textColor="#E52F31"
        />

</RelativeLayout>
